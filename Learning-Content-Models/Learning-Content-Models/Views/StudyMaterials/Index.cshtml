@model IEnumerable<Learning_Content_Models.Models.StudyMaterial>

<h2>StudyMaterials List</h2>
<table class="table">
	<thead>
		<tr>
			<th>Title</th>
			<th>Type</th>
			<th>Teacher</th>
			@*<th>Actors</th>*@
			<th>Url</th>
			<th>Description</th>
			<th>Subject</th>
			<th>CreateDate</th>
			<th>Class</th> 
			@*<th>Tickets</th>*@
			@* <th>Actions</th> *@
		</tr>
	</thead>
	<tbody>
		@foreach (var material in Model)
		{
			<tr>
				<td>@material.Title</td>
				<td>@material.Vid.Name</td>
			   <td>@material.ApplicationUser.Name</td>
				@*     <td>@string.Join(", ",movie.MovieActors
			.Select(a=>a.Actor.FirstName)
			.ToList())</td>*@
				<td><img src="@material.URL" alt="@material.Title" /></td>
				<td>@material.Description</td>
				<td>@material.Subject</td>
				<td>@material.CreateDate</td>
				<td>@material.Class</td>
				@*      <td>@string.Join(", ",movie.Tickets.Select
			(t=>t.Number).ToList())</td>*@
			</tr>
		}
	</tbody>
</table>
<a asp-action="Add" asp-controller="StudyMaterials" class="btn btn-success">Add New StudyMaterials</a>